{
  "name": "fllscoreclient",
  "version": "0.12.2",
  "description": "An implementation of the FLL Score Client Protocol.",
  "main": "dist/fllscoreclient.js",
  "types": "dist/lib/index.d.ts",
  "scripts": {
    "prepublishOnly": "./node_modules/.bin/webpack",
    "build": "./node_modules/.bin/webpack",
    "test": "./node_modules/.bin/mocha --require ts-node/register --require source-map-support/register ./test/**/*.spec.ts",
    "cover": "./node_modules/.bin/nyc npm run test",
    "coverage": "./node_modules/.bin/nyc npm run test && ./node_modules/.bin/nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Pr1sM/fllscoreclientjs.git"
  },
  "author": "Srinivas Dhanwada",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Pr1sM/fllscoreclientjs/issues"
  },
  "homepage": "https://github.com/Pr1sM/fllscoreclientjs#readme",
  "devDependencies": {
    "@types/chai": "^4.0.10",
    "@types/chai-datetime": "0.0.31",
    "@types/mocha": "^2.2.46",
    "@types/node": "^8.5.2",
    "@types/sinon": "^4.1.2",
    "@types/sinon-chai": "^2.7.29",
    "@types/socket.io": "^1.4.31",
    "@types/socket.io-client": "^1.4.32",
    "@types/ws": "^3.2.1",
    "chai": "^4.1.2",
    "chai-datetime": "^1.5.0",
    "coveralls": "^3.0.0",
    "json-loader": "^0.5.7",
    "mocha": "^4.1.0",
    "node-loader": "^0.6.0",
    "nyc": "^11.4.1",
    "sinon": "^4.1.3",
    "sinon-chai": "^2.14.0",
    "source-map-support": "^0.5.0",
    "transform-loader": "^0.2.4",
    "ts-loader": "^3.2.0",
    "ts-node": "^4.1.0",
    "tslint": "^5.8.0",
    "tslint-loader": "^3.5.3",
    "typedoc": "^0.9.0",
    "typedoc-plugin-markdown": "^1.0.12",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "events": "^1.1.1",
    "queue": "^4.4.2",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4",
    "ws": "^4.0.0"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "coverage/**",
      "dist/**",
      "example/**",
      "test/**",
      "node_modules/**",
      "webpack.config.js"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  }
}
